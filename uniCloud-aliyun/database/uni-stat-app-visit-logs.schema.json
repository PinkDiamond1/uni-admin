// 应用访问日志表
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"stat_app_id": {
			"bsonType": "string",
			"description": "应用表ID，对应opendb-app-list._id",
			"foreignKey": "opendb-app-list._id"
		},
		"stat_version_id": {
			"bsonType": "string",
			"description": "应用版本表ID，对应uni-stat-app-versions._id",
			"foreignKey": "uni-stat-app-versions._id"
		},
		"platform_id": {
			"bsonType": "string",
			"description": "平台编号",
			"foreignKey":"uni-stat-app-platforms._id"
		},
		"visitor_id": {
			"bsonType": "string",
			"description": "访问标示device_id|imei"
		},
		"visitor_returning": {
			"bsonType": "int",
			"description": "是否为新用户",
			"defaultValue": 0,
			"enum": [{
				"text": "否",
				"value": 0
			}, {
				"text": "是",
				"value": 1
			}]
		},
		"visitor_channel_returning": {
			"bsonType": "int",
			"description": "是否为当前渠道的新用户",
			"defaultValue": 0,
			"enum": [{
				"text": "否",
				"value": 0
			}, {
				"text": "是",
				"value": 1
			}]
		},
		"visitor_first_visit_date": {
			"bsonType": "timestamp",
			"description": "用户首次访问日期"
		},
		"visitor_channel_first_visit_date": {
			"bsonType": "timestamp",
			"description": "用户在渠道首次访问日期"
		},
		"visitor_count_visits": {
			"bsonType": "int",
			"description": "用户到本次为止的总访问次数"
		},
		"visitor_days_since_last": {
			"bsonType": "int",
			"description": "用户上次访问到本次访问间隔的天数"
		},
		"visitor_days_since_first": {
			"bsonType": "int",
			"description": "用户首次访问到本次访问间隔的天数"
		},
		"visit_first_action_time": {
			"bsonType": "timestamp",
			"description": "本次访问第一个行为发生的时间，行为包括页面跳转，事件等"
		},
		"visit_last_action_time": {
			"bsonType": "timestamp",
			"description": "本次访问最后一个行为发生的时间"
		},
		"visit_exit_page_id": {
			"bsonType": "string",
			"description": "本次访问退出的页面ID, 同uni-stat-app-pages._id"
		},
		"visit_exit_page_detail_id": {
			"bsonType": "string",
			"description": "本次访问退出的页面详情ID, 同uni-stat-app-page-detail._id"
		},
		"visit_entry_page_id": {
			"bsonType": "string",
			"description": "本次访问进入的页面ID, 同uni-stat-app-pages._id"
		},
		"visit_entry_page_detail_id": {
			"bsonType": "string",
			"description": "本次访问进入的页面详情ID, 同uni-stat-app-page-detail._id"
		},
		"visit_total_page_views": {
			"bsonType": "int",
			"description": "本次访问产生的点击次数"
		},
		"visit_total_events": {
			"bsonType": "int",
			"description": "本次访问产生的事件数"
		},
		"visit_total_time": {
			"bsonType": "int",
			"description": "本次访问的总时长，单位为秒"
		},
		"channel_id": {
			"bsonType": "string",
			"description": "渠道编号同uni-stat-app-channels._id"
		},
		"sub_channel_id": {
			"bsonType": "string",
			"description": ""
		},
		"sdk_version": {
			"bsonType": "string",
			"description": "sdk 版本"
		},
		"platform_version": {
			"bsonType": "string",
			"description": "平台版本，如ide、微信、支付宝等"
		},
		"device_os_id": {
			"bsonType": "int",
			"description": "设备系统编号"
		},
		"device_os_version": {
			"bsonType": "string",
			"description": "设备系统版本"
		},
		"device_net": {
			"bsonType": "int",
			"description": "设备网"
		},
		"device_vendor": {
			"bsonType": "string",
			"description": "设备供应商 "
		},
		"device_model": {
			"bsonType": "string",
			"description": "设备型号"
		},
		"device_language": {
			"bsonType": "string",
			"description": "设备语言包"
		},
		"device_pixel_ratio": {
			"bsonType": "string",
			"description": "设备像素比 "
		},
		"device_window_width": {
			"bsonType": "int",
			"description": "设备窗口宽度 "
		},
		"device_window_height": {
			"bsonType": "int",
			"description": "设备窗口高度"
		},
		"device_screen_width": {
			"bsonType": "int",
			"description": "设备屏幕宽度"
		},
		"device_screen_height": {
			"bsonType": "int",
			"description": "设备屏幕高度"
		},
		"location_ip": {
			"bsonType": "int",
			"description": "ip"
		},
		"location_latitude": {
			"bsonType": "double",
			"description": "纬度"
		},
		"location_longitude": {
			"bsonType": "double",
			"description": "经度"
		},
		"location_country": {
			"bsonType": "string",
			"description": "国家"
		},
		"location_province": {
			"bsonType": "string",
			"description": "省份"
		},
		"location_city": {
			"bsonType": "string",
			"description": "城市"
		},
		"create_time": {
			"bsonType": "timestamp",
			"description": "创建时间"
		}
	}
}
