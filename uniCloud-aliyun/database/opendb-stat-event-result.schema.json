// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"appid": {
			"bsonType": "string",
			"description": "应用ID"
		},
		"platform_id": {
			"bsonType": "string",
			"description": "应用平台ID，对应opendb-app-platforms._id	",
			"foreignKey": "opendb-app-platforms._id"
		},
		"channel_id": {
			"bsonType": "string",
			"description": "渠道/场景值ID，对应opendb-app-channels._id",
			"foreignKey": "opendb-app-channels._id"
		},
		"version_id": {
			"bsonType": "string",
			"description": "应用版本ID，对应opendb-stat-app-versions._id",
			"foreignKey": "opendb-stat-app-versions._id"
		},
		"event_id": {
			"bsonType": "string",
			"description": "事件ID, 同opendb-stat-events._id",
			"foreignKey":"opendb-stat-events._id"
		},
		"event_count": {
			"bsonType": "int",
			"description": "触发次数"
		},
		"user_count": {
			"bsonType": "int",
			"description": "触发该事件的用户数"
		},
		"stat_date":{
			"bsonType":"int",
			"description":"统计日期，格式yyyymmdd，例:20211201"
		},
		"start_time":{
			"bsonType":"timestamp",
			"description":"开始时间"
		},
		"end_time":{
			"bsonType":"timestamp",
			"description":"结束时间"
		}
	}
}
