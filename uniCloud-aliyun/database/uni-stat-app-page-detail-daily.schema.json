// uni统计-应用详情页面访问数据日统计表
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"stat_app_id": {
			"bsonType": "string",
			"description": "应用表ID，对应opendb-app-list._id",
			"foreignKey": "opendb-app-list._id"
		},
		"stat_version_id": {
			"bsonType": "string",
			"description": "应用版本表ID，对应uni-stat-app-versions._id",
			"foreignKey": "uni-stat-app-versions._id"
		},
		"platform_id": {
			"bsonType": "string",
			"description": "平台编号",
			"foreignKey":"uni-stat-app-platforms._id"
		},
		"channel_id": {
			"bsonType": "string",
			"description": "渠道编号同uni-stat-app-channels._id",
			"foreignKey": "uni-stat-app-channels._id"
		},
		"page_detail_id": {
			"bsonType": "string",
			"description": "页面详情ID, 同uni-stat-app-page-detail._id",
			"foreignKey": "uni-stat-app-page-detail._id"
		},
		"num_visits": {
			"bsonType": "int",
			"description": "访问次数"
		},
		"num_visitor": {
			"bsonType": "int",
			"description": "访问用户数"
		},
		"exit_num_visits": {
			"bsonType": "int",
			"description": "退出次数"
		},
		"sum_visit_length": {
			"bsonType": "int",
			"description": "访问总时长,单位秒"
		},
		"num_share": {
			"bsonType": "int",
			"description": "分享次数"
		},
		"entry_num_visitor": {
			"bsonType": "int",
			"description": "入口页访问人数"
		},
		"entry_num_visits": {
			"bsonType": "int",
			"description": "入口页访问次数"
		},
		"entry_sum_visit_length": {
			"bsonType": "int",
			"description": "入口页访问总时长,单位秒"
		},
		"stat_date": {
			"bsonType": "int",
			"description": "统计日期，格式yyyymmdd，例:20211201"
		}
	}
}