// 每日访问情况汇总表
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"stat_app_id": {
			"bsonType": "string",
			"description": "应用表ID，对应opendb-app-list._id",
			"foreignKey": "opendb-app-list._id"
		},
		"platform_id": {
			"bsonType": "string",
			"description": "平台编号",
			"foreignKey": "uni-stat-app-platforms._id"
		},
		"channel_id": {
			"bsonType": "string",
			"description": "渠道编号同uni-stat-app-channels._id",
			"foreignKey": "uni-stat-app-channels._id"
		},
		"num_new_visitor": {
			"bsonType": "int",
			"description": "新增用户数"
		},
		"num_visitor": {
			"bsonType": "int",
			"description": "活跃用户数"
		},
		"num_page_views": {
			"bsonType": "int",
			"description": "访问次数"
		},
		"open_num": {
			"bsonType":"int",
			"description":"打开次数"
		},
		"num_total_visitor": {
			"bsonType": "int",
			"description": "总用户数"
		},
		"num_errors": {
			"bsonType": "int",
			"description": "报错次数"
		},
		"num_visits": {
			"bsonType": "int",
			"description": "应用启动次数"
		},
		"visit_avg_time": {
			"bsonType": "int",
			"defaultValue": "次均停留时长"
		},
		"visitor_avg_time": {
			"bsonType": "int",
			"description": "人均停留时长"
		},
		"bounce_rate":{
			"bsonType":"double",
			"description":"跳出率"
		},
		"stat_date": {
			"bsonType": "int",
			"description": "统计日期，格式yyyymmdd，例:20211201"
		}
	}
}
