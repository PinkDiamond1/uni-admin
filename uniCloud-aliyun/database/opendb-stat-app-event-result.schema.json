// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"appid": {
			"bsonType": "string",
			"description": "应用表ID，对应opendb-app-list._id",
			"foreignKey": "opendb-app-list._id"
		},
		"platform_id": {
			"bsonType": "string",
			"description": "应用平台ID，对应opendb-stat-app-platforms._id	",
			"foreignKey": "opendb-stat-app-platforms._id"
		},
		"channel_id": {
			"bsonType": "string",
			"description": "渠道/场景值ID，对应opendb-stat-app-channels._id",
			"foreignKey": "opendb-stat-app-channels._id"
		},
		"version_id": {
			"bsonType": "string",
			"description": "应用版本ID，对应opendb-stat-app-versions._id",
			"foreignKey": "opendb-stat-app-versions._id"
		},
		"event_id": {
			"bsonType": "string",
			"description": "事件ID, 同opendb-stat-app-events._id",
			"foreignKey":"opendb-stat-app-events._id"
		},
		"trigger_count": {
			"bsonType": "int",
			"description": "触发次数"
		},
		"trigger_users": {
			"bsonType": "int",
			"description": "触发该事件的用户数"
		},
		"dimension": {
			"bsonType": "string",
			"description": "统计范围 day:按天统计，hour:按小时统计"
		},
		"stat_date":{
			"bsonType":"int",
			"description":"统计日期，格式yyyymmdd，例:20211201"
		},
		"start_time":{
			"bsonType":"timestamp",
			"description":"开始时间"
		},
		"end_time":{
			"bsonType":"timestamp",
			"description":"结束时间"
		}
	}
}