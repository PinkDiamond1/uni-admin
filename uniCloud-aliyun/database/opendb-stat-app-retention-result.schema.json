// uni统计-留存数据日统计表
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"session_result_id": {
			"bsonType": "string",
			"description":"统计ID，对应opendb-stat-app-session-result._id",
			"foreignKey": "opendb-stat-app-session-result._id"
		},
		"days": {
			"bsonType": "int",
			"description": "N日留存，代表N"
		},
		"active_users": {
			"bsonType": "int",
			"description": "活跃用户留存数"
		},
		"new_users": {
			"bsonType": "int",
			"description": "新增用户留存数"
		},
		"active_user_rate": {
			"bsonType": "int",
			"description": "活跃用户留存率"
		},
		"new_user_rate": {
			"bsonType": "int",
			"description": "新增用户留存率"
		},
		"stat_date": {
			"bsonType": "int",
			"description": "统计日期，格式yyyymmdd，例:20211201"
		},
		"start_time":{
			"bsonType":"timestamp",
			"description":"开始时间"
		},
		"end_time":{
			"bsonType":"timestamp",
			"description":"结束时间"
		}
	}
}