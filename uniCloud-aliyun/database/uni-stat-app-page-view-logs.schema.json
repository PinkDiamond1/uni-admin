// 应用页面访问日志
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"stat_app_id": {
			"bsonType": "string",
			"description": "应用表ID，对应opendb-app-list._id",
			"foreignKey": "opendb-app-list._id"
		},
		"channel_id": {
			"bsonType": "string",
			"description": "渠道编号同uni-stat-app-channels._id",
			"foreignKey": "uni-stat-app-channels._id"
		},
		"stat_version_id": {
			"bsonType": "string",
			"description": "应用版本表ID，对应uni-stat-app-versions._id",
			"foreignKey": "uni-stat-app-versions._id"
		},
		"stat_visit_id": {
			"bsonType": "string",
			"description": "访问日志ID",
			"foreignKey": "uni-stat-app-visit-logs._id"
		},
		"platform_id": {
			"bsonType": "string",
			"description": "平台编号",
			"foreignKey":"uni-stat-app-platforms._id"
		},
		"visitor_id": {
			"bsonType": "string",
			"description": "访问标示device_id|imei"
		},
		"page_id": {
			"bsonType": "string",
			"description": "页面ID, 同uni-stat-app-pages._id"
		},
		"page_ref_id": {
			"bsonType": "string",
			"description": "上个页面的 id，为0表示第一个页面, 同uni-stat-app-pages._id"
		},
		"time_spent_ref_page": {
			"bsonType": "int",
			"description": "上个页面停留时间"
		},
		"page_detail_id": {
			"bsonType": "string",
			"description": "页面详情ID, 同uni-stat-app-page-detail._id"
		},
		"page_detail_ref_id": {
			"bsonType": "string",
			"description": "上个页面详情的 id，为0表示第一个页面, 同uni-stat-app-page-detail._id"
		},
		"is_ref_entry": {
			"bsonType": "int",
			"defaultValue": 0,
			"description": "是否由上级页面进入"
		},
		"create_time": {
			"bsonType": "timestamp",
			"description": "创建时间"
		}
	}
}
