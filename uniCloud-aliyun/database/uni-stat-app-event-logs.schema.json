// 应用事件日志
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"stat_app_id": {
			"bsonType": "string",
			"description": "应用表ID，对应opendb-app-list._id",
			"foreignKey": "opendb-app-list._id"
		},
		"channel_id": {
			"bsonType": "string",
			"description": "渠道编号同uni-stat-app-channels._id",
			"foreignKey": "uni-stat-app-channels._id"
		},
		"stat_version_id": {
			"bsonType": "string",
			"description": "应用版本表ID，对应uni-stat-app-versions._id",
			"foreignKey": "uni-stat-app-versions._id"
		},
		"stat_visit_id": {
			"bsonType": "string",
			"description": "访问日志ID",
			"foreignKey": "uni-stat-app-visit-logs._id"
		},
		"platform_id": {
			"bsonType": "string",
			"description": "平台编号",
			"foreignKey":"uni-stat-app-platforms._id"
		},
		"visitor_id": {
			"bsonType": "string",
			"description": "访问标示device_id|imei"
		},
		"page_id": {
			"bsonType": "string",
			"description": "页面ID, 同uni-stat-app-pages._id"
		},
		"event_id": {
			"bsonType": "string",
			"description": "事件ID, 同uni-stat-app-events._id"
		},
		"custom_var": {
			"bsonType": "string",
			"description": "客户端标识"
		},
		"create_time": {
			"bsonType": "timestamp",
			"description": "创建时间"
		}
	}
}
